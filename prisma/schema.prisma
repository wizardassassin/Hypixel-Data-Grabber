generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model BazaarTimeLog {
  lastUpdated     BigInt @id
  totalSellVolume Int
  totalBuyVolume  Int
  totalSellOrders Int
  totalBuyOrders  Int
  data            Json
}

model AuctionTimeLog {
  lastUpdated      BigInt @id
  totalAuctions    Int
  totalBinAuctions Int
  totalPages       Int
  data             Json
}

model EndedAuctionTimeLog {
  lastUpdated   BigInt @id
  totalAuctions Int
  data          Json
}

model PlayerTimeLog {
  lastUpdated BigInt @id
  playerCount Int
  data        Json
}

model BoosterTimeLog {
  lastUpdated    BigInt  @id
  isDecrementing Boolean
  data           Json
}

model LeaderboardsTimeLog {
  lastUpdated BigInt @id
  data        Json
}

model WatchdogTimeLog {
  lastUpdated BigInt @id
  data        Json
}
